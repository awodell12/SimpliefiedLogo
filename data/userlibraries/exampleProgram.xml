<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<UserLib>
  <CommandsList>
    <command contents="penup fd :firstvar pd " name="jump">
      <argument label="firstvar"/>
    </command>
    <command contents="repeat 3 [ fd :firstvar rt 60 jump :firstvar rt 60 ] " name="Hex">
      <argument label="firstvar"/>
    </command>
    <command contents="make dist * :z :y Hex :dist" name="partOne">
    </command>
    <command contents="setpc 4 jump :dist rt 60 Hex :dist" name = "partTwo">
    </command>
    <command contents="partOne partTwo" name="RunProgram"></command>
  </CommandsList>
  <VariableList>
    <Variable name="x" value="5.0"/>
    <Variable name="y" value="3.0"/>
    <Variable name="z" value="23.1"/>
  </VariableList>
</UserLib>
